<savedqueries>
  <!-- Available Spaces View -->
  <savedquery>
    <savedqueryid>{00000000-0000-0000-00AA-000010001005}</savedqueryid>
    <name>Available Spaces</name>
    <querytype>0</querytype>
    <isdefault>1</isdefault>
    <returnedtypecode>10002</returnedtypecode>
    <isquickfindquery>0</isquickfindquery>
    <isprivate>0</isprivate>
    <fetchxml>
      <fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="false">
        <entity name="es_space">
          <attribute name="es_name" />
          <attribute name="es_marketid" />
          <attribute name="es_spacetype" />
          <attribute name="es_floor" />
          <attribute name="es_roomnumber" />
          <attribute name="es_maxcapacity" />
          <attribute name="es_dailyrate" />
          <attribute name="es_status" />
          <attribute name="es_wifi" />
          <attribute name="es_videoconferencing" />
          <attribute name="es_spaceid" />
          <order attribute="es_name" descending="false" />
          <filter type="and">
            <condition attribute="es_status" operator="eq" value="1" />
            <condition attribute="statecode" operator="eq" value="0" />
          </filter>
          <link-entity name="es_market" from="es_marketid" to="es_marketid" link-type="inner" alias="market">
            <attribute name="es_name" />
            <attribute name="es_city" />
            <attribute name="es_state" />
          </link-entity>
        </entity>
      </fetch>
    </fetchxml>
    <layoutxml>
      <grid name="resultset" jump="es_name" select="1" icon="1" preview="1">
        <row name="result" id="es_spaceid">
          <cell name="es_name" width="150" />
          <cell name="market.es_name" width="150" disableSorting="0" />
          <cell name="market.es_city" width="100" disableSorting="0" />
          <cell name="es_spacetype" width="100" />
          <cell name="es_floor" width="60" />
          <cell name="es_roomnumber" width="80" />
          <cell name="es_maxcapacity" width="80" />
          <cell name="es_dailyrate" width="100" />
          <cell name="es_wifi" width="60" />
          <cell name="es_videoconferencing" width="60" />
          <cell name="es_status" width="100" />
        </row>
      </grid>
    </layoutxml>
    <LocalizedNames>
      <LocalizedName description="Available Spaces" languagecode="1033" />
    </LocalizedNames>
    <Descriptions>
      <Description description="Shows all available spaces" languagecode="1033" />
    </Descriptions>
  </savedquery>
  
